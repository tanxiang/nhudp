                android:key="encMethod1"
                android:entries="@array/test"
                android:entryValues="@array/test"
                android:title="@string/test">
        </org.ttand.nhudp.preferences>
    </PreferenceCategory>

    <PreferenceCategory
            android:title="@string/proxy_config">
        <org.ttand.nhudp.preferences
                android:defaultValue="1"
                android:key="encMethod2"
                android:entries="@array/test"
                android:entryValues="@array/test"
                android:title="@string/test">
        </org.ttand.nhudp.preferences>
    </PreferenceCategory>

</PreferenceScreen>
